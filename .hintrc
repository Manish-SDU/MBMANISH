{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ],
    "disown-opener": "off",
    "no-inline-styles": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "hyphens",
          "-webkit-text-size-adjust",
          "font-size: xxx-large"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 133",
    "not and_ff <= 135",
    "not chrome <= 133",
    "not edge <= 133",
    "not firefox <= 135",
    "not ios_saf <= 18.3",
    "not opera <= 114",
    "not safari <= 18.3",
    "not samsung <= 27"
  ]
}